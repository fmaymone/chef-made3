<div class="container">
  <div class="row">
  <%= form_for(resource, as: resource_name,  url: registration_path(resource_name), html: { :class => 'col-md-9 go-right', multipart: true ,method: :put }) do |f| %>
    <%= render 'shared/devisemes' %>

    <div class="form-group">
       <%= f.label :cpf, "CPF:" %>
      <%= f.text_field :cpf, autofocus: true,  :class => 'form-control' %>
    </div>
   
    <div class="form-group">
       <%= f.label :rg, "RG:" %>
      <%= f.text_field :rg, autofocus: true,  :class => 'form-control' %>
    </div>
 

    <div class="form-group">
      <%= f.text_area :miniCurriculo, :placeholder => "Mini-Curriculo", :class => 'form-control' %>
    </div>
    
    <div class="row">
					<div class="col-md-4">
						<div class="form-group">							
							<span class="btn btn-default btn-file">
							    <i class="fa fa-cloud-upload fa-lg"></i> Upload Photos 
							    <%= file_field_tag "images[]", type: :file, multiple: true %>
							</span>
						</div>		
					</div>
				</div>

		<div id="photos"><%= render 'photos/list' %></div>


  


    
    <div class="actions">
      <%= f.submit "Salvar", :class => "btn btn-primary" %>
    </div>
  <% end %>
  </div>
</div>
